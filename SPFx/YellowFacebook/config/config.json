{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "yellow-facebook-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/yellowFacebook/YellowFacebookWebPart.js",
          "manifest": "./src/webparts/yellowFacebook/YellowFacebookWebPart.manifest.json"
        }
      ]
    },
    "yellow-profile-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/yellowProfile/YellowProfileWebPart.js",
          "manifest": "./src/webparts/yellowProfile/YellowProfileWebPart.manifest.json"
        }
      ]
    },
    "yellow-facebook-top-bar-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/yellowFacebookTopBar/YellowFacebookTopBarApplicationCustomizer.js",
          "manifest": "./src/extensions/yellowFacebookTopBar/YellowFacebookTopBarApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "YellowFacebookWebPartStrings": "lib/webparts/yellowFacebook/loc/{locale}.js",
    "YellowProfileWebPartStrings": "lib/webparts/yellowProfile/loc/{locale}.js",
    "YellowFacebookTopBarApplicationCustomizerStrings": "lib/extensions/yellowFacebookTopBar/loc/{locale}.js"
  }
}
